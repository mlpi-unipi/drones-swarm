<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="default">
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <model name="ball">
      <link name="link">
        <inertial>
          <mass>0.026</mass>
          <!-- inertia based on solid sphere 2/5 mr^2 -->
          <inertia>
            <ixx>1.664e-5</ixx>
            <iyy>1.664e-5</iyy>
            <izz>1.664e-5</izz>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyz>0</iyz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <sphere>
              <radius>0.04</radius>
            </sphere>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <sphere>
              <radius>0.04</radius>
            </sphere>
          </geometry>
        </visual>
        <sensor type="contact" name="bumper">
          <update_rate>-1</update_rate>
          <contact>
            <collision>collision</collision>
          </contact>
          <plugin name="contact_sensor" filename="libgazebo_ros_bumper.so">
            <ros>
              <namespace>test</namespace>
              <remapping>bumper_states:=bumper_test</remapping>
            </ros>
            <frame_name>world</frame_name>
          </plugin>
        </sensor>
      </link>
    </model>
  </world>
</sdf>
