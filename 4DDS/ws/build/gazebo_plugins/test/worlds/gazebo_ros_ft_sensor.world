<?xml version="1.0"?>
<sdf version="1.6">
  <world name="default">
    <include>
      <uri>model://sun</uri>
    </include>
    <model name="box">
      <pose>0 0 0 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </visual>
      </link>
      <plugin name="gazebo_ros_ft_sensor" filename="libgazebo_ros_ft_sensor.so">
        <ros>
          <namespace>/test</namespace>
          <remapping>wrench:=ft_sensor_test</remapping>
        </ros>
        <body_name>link</body_name>
        <frame_name>test_world</frame_name>
      </plugin>
    </model>
  </world>
</sdf>
